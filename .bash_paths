# Go
export GOPATH=$HOME/go
export GOBIN=$GOPATH/bin

# Rvm
export RVMPATH=$HOME/.rvm

# Pyenv
export PYENV_ROOT=$HOME/.pyenv

# Lang?
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:"/usr/local/lib"

# Android Studio
export ANDROID_HOME=/Users/john/Library/Android/sdk

# Ripgrep
export RIPGREP_CONFIG_PATH=$HOME/.ripgreprc

# Nvm
export NVM_DIR="$HOME/.nvm"

export PATH=$PATH:$HOME/bin:$HOME/.local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/snap/bin:/usr/local/go/bin:$GOBIN:$HOME/.pyenv/bin:$HOME/.rvm/bin
